{
  "name": "winner-winner-chicken-dinner",
  "version": "1.0.0",
  "description": "Voting app built on elm-pages",
  "scripts": {
    "// Production Scripts": null,
    "serve": "run-s build server",
    "server": "http-server ./dist -a localhost -p 3000 -c-1",
    "build": "run-s gen elm",
    "elm": "elm-pages build --debug --keep-cache",
    "// Dev Scripts": null,
    "start": "elm-pages dev",
    "build:dev": "env-cmd run-s gen elm",
    "// Code Gen": null,
    "gen": "run-p gen:graphql gen:tailwind",
    "gen:tailwind": "elm-tailwind-modules --dir ./gen --tailwind-config tailwind.config.js",
    "gen:graphql": "elm-graphql https://smoothie-shop.hasura.app/v1/graphql --header \"x-hasura-admin-secret: $SMOOTHIES_HASURA_SECRET\" --output gen"
  },
  "author": "Dillon Kearns",
  "license": "BSD-3",
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^4.2.3",
    "@netlify/functions": "^0.7.2",
    "@tailwindcss/forms": "^0.3.4",
    "busboy": "^1.1.0",
    "elm-format": "^0.8.5",
    "elm-pages": "file:elm-pages",
    "elm-review": "^2.7.0",
    "elm-tailwind-modules": "^0.3.2",
    "elm-tooling": "^1.3.0",
    "env-cmd": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prisma": "^3.15.2",
    "tailwindcss": "^2.2.19"
  }
}
